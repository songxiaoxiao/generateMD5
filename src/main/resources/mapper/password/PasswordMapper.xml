<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="moxi.core.demo.dao.password.PasswordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="moxi.core.demo.model.password.Password">
        <id column="id" property="id" />
        <result column="laws" property="laws" />
        <result column="cipher" property="cipher" />
        <result column="originate" property="originate" />
        <result column="remark" property="remark" />

    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,laws,cipher,originate,remark
    </sql>

</mapper>
